{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "1.0.24",
  "type": "module",
  "author": "Ebdaa Digital Technology",
  "description": "Ebdaa Work Time - Employee time tracking and productivity monitoring application",
  "overrides": {
    "nan": "2.16.0"
  },
  "main": "build/electron/electron/main.cjs",
  "scripts": {
    "start": "SCREENSHOT_INTERVAL_SECONDS=20 concurrently \"npm:dev\" \"npm:electron\"",
    "start:desktop": "npm run build:all && npx electron build/electron/electron/main.cjs",
    "electron": "SCREENSHOT_INTERVAL_SECONDS=20 npm run build:electron && SCREENSHOT_INTERVAL_SECONDS=20 npx electron build/electron/electron/main.cjs",
    "dev:electron": "ts-node-esm --project tsconfig.electron.json electron/main.ts",
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "electron:build": "electron-builder",
    "electron:build-unsigned": "electron-builder --config.mac.identity=null --config.mac.notarize=null",
    "build:web": "tsc -p tsconfig.app.json",
    "build:electron": "mkdir -p build/electron && tsc -p tsconfig.electron.json && find build/electron -name '*.js' -exec sh -c 'mv \"$1\" \"${1%.js}.cjs\"' _ {} \\; && node fix-imports.cjs && mkdir -p build/desktop-agent && cp -r desktop-agent/renderer desktop-agent/config.json desktop-agent/assets desktop-agent/debug-window.html build/desktop-agent/ && mkdir -p build/electron/assets && cp -r electron/assets/* build/electron/assets/ && cp public/app-update.yml build/electron/ && cp build/electron/electron/env-config.js build/electron/electron/ 2>/dev/null || echo 'env-config.js will be created during build'",
    "build:all": "npm run build:dev && npm run build:electron && mkdir -p build/dist && cp -r dist/* build/dist/",
    "build:notarized": "scripts/build-notarized.sh",
    "build:release": "scripts/build-and-release.sh",
    "build:signed": "npx electron-builder --mac --win --publish=never",
    "setup:signing": "./scripts/setup-code-signing.sh",
    "install:certificates": "./scripts/install-certificates.sh",
    "deploy": "./scripts/build-and-deploy.sh",
    "update:links": "./scripts/update-web-links.sh",
    "test:windows": "node test-windows-compatibility.cjs",
    "test:cross-platform": "node test-windows-compatibility.cjs"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-context-menu": "^2.2.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@supabase/supabase-js": "^2.49.8",
    "@tanstack/react-query": "^5.56.2",
    "@types/react-big-calendar": "^1.16.1",
    "active-win": "^8.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "electron-auto-launch": "^5.0.1",
    "electron-updater": "^6.6.2",
    "embla-carousel-react": "^8.3.0",
    "glob": "^11.0.3",
    "input-otp": "^1.2.4",
    "lucide-react": "^0.462.0",
    "nanoid": "^5.0.6",
    "next-themes": "^0.3.0",
    "node-global-key-listener": "^0.3.0",
    "react": "^18.3.1",
    "react-big-calendar": "^1.18.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-resizable-panels": "^2.1.3",
    "react-router-dom": "^6.26.2",
    "recharts": "^2.12.7",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@electron/notarize": "^2.5.0",
    "@eslint/js": "^9.9.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/electron": "^1.4.38",
    "@types/node": "^22.15.21",
    "@types/react": "^18.3.22",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.0.1",
    "electron": "^28.3.3",
    "electron-builder": "^24.13.2",
    "electron-rebuild": "^3.2.9",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.15.0",
    "lovable-tagger": "^1.1.7",
    "postcss": "^8.4.47",
    "supabase": "^2.23.4",
    "tailwindcss": "^3.4.11",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1"
  },
  "build": {
    "appId": "com.ebdaadt.timetracker",
    "productName": "Ebdaa Work Time",
    "icon": "electron/assets/icon.png",
    "directories": {
      "output": "dist",
      "buildResources": "build"
    },
    "publish": {
      "provider": "github",
      "owner": "mafatah",
      "repo": "time-flow-admin"
    },
    "files": [
      "build/electron/**/*",
      "build/desktop-agent/**/*",
      "!build/entitlements.mac.plist",
      "!build/dist/**/*"
    ],
    "extraResources": [
      {
        "from": "build/dist",
        "to": "app",
        "filter": [
          "**/*",
          "!downloads/**/*",
          "!*.dmg",
          "!*.blockmap"
        ]
      }
    ],
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": "arm64"
        },
        {
          "target": "dmg",
          "arch": "x64"
        }
      ],
      "category": "public.app-category.productivity",
      "identity": "Ebdaa Digital Technology (6GW49LK9V9)",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "icon": "electron/assets/icon.png",
      "notarize": {
        "teamId": "6GW49LK9V9"
      }
    },
    "dmg": {
      "format": "UDZO",
      "icon": "electron/assets/icon.png",
      "iconSize": 80,
      "title": "Install Ebdaa Work Time",
      "window": {
        "width": 600,
        "height": 420
      },
      "contents": [
        {
          "x": 150,
          "y": 220,
          "type": "file"
        },
        {
          "x": 450,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": {
        "target": "nsis",
        "arch": [
          "x64"
        ]
      },
      "icon": "electron/assets/icon.png",
      "signingHashAlgorithms": [
        "sha256"
      ],
      "publisherName": "Ebdaa Digital Technology"
    },
    "linux": {
      "target": {
        "target": "AppImage",
        "arch": [
          "x64"
        ]
      },
      "category": "Office",
      "icon": "electron/assets/icon.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
