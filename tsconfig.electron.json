{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ES2020", "DOM"],
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "rootDir": "./",
    "outDir": "./",
    "noEmit": false,
    "types": []
  },
  "include": ["electron/**/*.ts"],
  "files": ["electron/shims.d.ts"]
}
